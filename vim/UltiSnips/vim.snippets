priority -1

snippet fn
function! ${1:Name}() abort
  $0
endfunction
endsnippet

snippet fnd
function! ${1:Dict}.${2:Name}() abort dict
  $0
endfunction
endsnippet

snippet fna
function! `!v expand('%:p')[stridx(expand('%:p'), '/autoload/')+len('/autoload/'):]->substitute('.vim$', '', '')->substitute('/', '#', 'g')`#${1:Name}() abort
  $0
endfunction
endsnippet

snippet wh
while $1
  $0
endwhile
endsnippet

snippet if
if $1
  $0
endif
endsnippet
